{
  "consumer": {
    "name": "urlShortener-client"
  },
  "interactions": [
    {
      "description": "a request for a greeting",
      "request": {
        "headers": {
          "Accept": "*/*"
        },
        "method": "GET",
        "path": "/api"
      },
      "response": {
        "body": {
          "greeting": "hello world"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.greeting": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "status": {}
        },
        "status": 200
      }
    },
    {
      "description": "a request for a url",
      "providerStates": [
        {
          "name": "a url is saved and clicked once"
        }
      ],
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/api/urls/googleId1"
      },
      "response": {
        "body": {
          "longUrl": "https://google.com",
          "shortUrl": "https://sh.rt/googleId1",
          "totalClicks": 1
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.longUrl": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.shortUrl": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.totalClicks": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "number"
                }
              ]
            }
          },
          "status": {}
        },
        "status": 200
      }
    },
    {
      "description": "a request for a url with an invalid id",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/api/urls/invalid-id"
      },
      "response": {
        "body": {
          "message": "Id is invalid"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.message": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "status": {}
        },
        "status": 400
      }
    },
    {
      "description": "a request for the top devices that clicked the url",
      "providerStates": [
        {
          "name": "a url is saved and clicked once"
        }
      ],
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/api/urls/googleId1/top-device-types"
      },
      "response": {
        "body": {
          "devices": [
            {
              "percentage": 1,
              "type": "tablet"
            }
          ]
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.devices[0].percentage": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "number"
                }
              ]
            },
            "$.devices[0].type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "status": {}
        },
        "status": 200
      }
    },
    {
      "description": "a request for total clicks by day",
      "providerStates": [
        {
          "name": "a url is saved and clicked once"
        }
      ],
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/api/urls/googleId1/total-clicks-by-day"
      },
      "response": {
        "body": {
          "dailyClickCounts": [
            {
              "day": "1/1/1999",
              "totalClicks": 1
            }
          ],
          "totalClicks": 1
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.dailyClickCounts[0].day": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.dailyClickCounts[0].totalClicks": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "number"
                }
              ]
            },
            "$.totalClicks": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "number"
                }
              ]
            }
          },
          "status": {}
        },
        "status": 200
      }
    },
    {
      "description": "a request to shorten a url",
      "request": {
        "body": {
          "url": "https://google.com"
        },
        "headers": {
          "Accept": "application/json",
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.url": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "method": "POST",
        "path": "/api/urls"
      },
      "response": {
        "body": {
          "longUrl": "https://google.com",
          "shortUrl": "https://sh.rt/googleId1"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.longUrl": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.shortUrl": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "status": {}
        },
        "status": 201
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "13.2.0"
    },
    "pactRust": {
      "ffi": "0.4.22",
      "models": "1.2.3"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "urlShortener-server"
  }
}